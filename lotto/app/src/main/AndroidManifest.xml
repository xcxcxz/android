<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.lotto">

    <application
        android:allowBackup="true"
        android:icon="@drawable/money"
        android:label="@string/app_name"
        android:roundIcon="@drawable/money"
        android:supportsRtl="true"
        android:theme="@style/Theme.Lotto">
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".ConstellationActivity"
            android:theme="@style/AppTheme.Constellation" />
        <activity
            android:name=".NameActivity"
            android:theme="@style/AppTheme.Name" />
        <activity
            android:name=".ResultActivity"
            android:theme="@style/AppTheme.Result"/>
        <activity android:name=".TestActivity"/>
        <activity android:name=".AbsoluteActivity"/>
        <activity android:name=".ConstarintLayoutActivity"/>


    </application>

</manifest>